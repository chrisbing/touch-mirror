{
  "name": "server",
  "version": "0.0.1",
  "description": "server",
  "license": "MIT",
  "scripts": {
    "server-dev": "NODE_ENV=development yarn build:test && concurrently --kill-others -n ts,server \"yarn watch-ts\" \"yarn watch-server\"",
    "watch-server": "NODE_ENV=development nodemon dist/server/app.js",
    "watch-ts": "tsc -w",
    "build:test": "NODE_ENV=development tsc",
    "build": "NODE_ENV=production tsc",
    "server:test": "NODE_ENV=development node dist/server/app.js",
    "server": "NODE_ENV=production node dist/server/app.js"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@types/crypto-js": "^3.1.43",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "^8.0.0",
    "@types/ioredis": "^4.0.13",
    "@types/koa": "^2.0.49",
    "@types/koa-router": "^7.0.42",
    "@types/koa-session": "^5.10.1",
    "@types/koa-static": "^4.0.1",
    "@types/koa__cors": "^2.2.3",
    "@types/lodash": "^4.14.136",
    "@types/lru-cache": "^5.1.0",
    "@types/mongoose": "^5.5.9",
    "@types/mongoose-sequence": "^3.0.3",
    "@types/qs": "^6.5.3",
    "@types/semver": "^6.0.1",
    "axios": "^0.19.0",
    "bsdiff-nodejs": "^2.0.7",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.8.15",
    "form-data": "^2.5.0",
    "fs-extra": "^8.1.0",
    "hasha": "^5.0.0",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.2",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.14",
    "lru-cache": "^5.1.1",
    "mongoose": "^5.6.6",
    "mongoose-sequence": "^5.1.0",
    "qs": "^6.7.0",
    "rss-parser": "^3.7.3",
    "semver": "^6.3.0"
  },
  "include": [
    "./types/**/*"
  ]
}
